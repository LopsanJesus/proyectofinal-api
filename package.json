{
  "name": "treelang-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "local": "nodemon",
    "start": "node ./bin/www",
    "db:update": "npm run migrate && npm run seed",
    "migrate": "node migrate.js",
    "seed": "node seed.js",
    "db:update:prod": "npm run migrate:prod && npm run seed:prod",
    "migrate:prod": "heroku run node migrate.js -a treelang-api",
    "seed:prod": "heroku run node seed.js -a treelang-api"
  },
  "dependencies": {
    "apollo-server-express": "^2.18.1",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.4",
    "pg": "^8.4.1",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0"
  }
}
